{
  "name": "@side/npm-dependency-stats-action",
  "version": "0.0.0-development",
  "description": "Github action for getting stats about npm dependencies",
  "author": "Side Inc.",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint src/**/*.ts",
    "build": "rimraf dist && ncc build src/index.ts --license licenses.txt",
    "test": "yarn tsc && jest",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "semver": "7.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@side/commitlint-config": "1.0.1",
    "@side/eslint-config-base": "2.0.2",
    "@side/eslint-config-jest": "1.0.0",
    "@side/prettier-config": "1.0.0",
    "@tsconfig/node16": "16.1.1",
    "@types/jest": "29.5.12",
    "@types/node": "16.18.78",
    "@types/semver": "7.5.6",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "48.0.4",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "13.3.0",
    "prettier": "3.2.2",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,yaml,yml,json,html,css,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reside-eng/npm-dependency-stats-action.git"
  },
  "prettier": "@side/prettier-config",
  "packageManager": "yarn@3.8.0"
}
